{
    "extends": "airbnb",
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": [2, 4],
        "react/jsx-indent": [2, 4],
        "no-console": 0,
        "quotes": 0,
        "brace-style": [1, "stroustrup"],
        "react/prop-types": 0,
        "new-cap": 0,
        "react/jsx-indent-props": [2, 4]
    }
}
